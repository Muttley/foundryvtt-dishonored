{
	"id": "FVTT-Dishonored",
	"title": "Dishonored: The Roleplaying Game",
	"description": "An unofficial system for playing the Dishonored RPG from Modiphius Entertainment Ltd.",
	"version": "1.3.0",
	"compatibility": {
		"minimum": "12",
		"verified": "12"
	},
	"authors": [
		{
			"name": "Paul Maskelyne",
			"discord": "muttley",
			"email": "mu77ley@gmail.com",
			"url": "https://github.com/Muttley"
		},
		{
			"name": "KaitoR95",
			"discord": "KaitoR (カーティス)#2355",
			"email": "curtisreet95@gmail.com",
			"url": "https://github.com/CurtR95"
		}
	],
	"esmodules": [
		"dishonored-compiled.mjs"
	],
	"styles": [
		"css/dishonored.css"
	],
	"packs": [
		{
			"name": "belongings",
			"label": "Belongings",
			"type": "Item",
			"path": "./packs/belongings",
			"private": false,
			"flags": {}
		},
		{
			"name": "focuses",
			"label": "Focuses",
			"type": "Item",
			"path": "./packs/focuses",
			"private": false,
			"flags": {}
		},
		{
			"name": "powers",
			"label": "Powers and Enhancements",
			"type": "Item",
			"path": "./packs/powers",
			"private": false,
			"flags": {}
		},
		{
			"name": "talents",
			"label": "Talents",
			"type": "Item",
			"path": "./packs/talents",
			"private": false,
			"flags": {}
		},
		{
			"name": "system-documentation",
			"label": "System Documentation",
			"path": "packs/system-documentation",
			"type": "JournalEntry",
			"private": false,
			"flags": {}
		}
	],
	"packFolders": [
		{
			"name": "Dishonored RPG",
			"sorting": "m",
			"color": "#2e3830",
			"packs": [
				"belongings",
				"focuses",
				"powers",
				"talents",
				"system-documentation"
			]
		}
	],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "i18n/en.json"
		},
		{
			"lang": "es",
			"name": "Spanish",
			"path": "i18n/es.json"
		},
		{
			"lang": "it",
			"name": "Italian",
			"path": "i18n/it.json"
		}
	],
	"flags": {
		"hotReload": {
			"extensions": [
				"css", "hbs", "json"
			],
			"paths": [
				"css",
				"i18n",
				"templates"
			]
		}
	},
	"gridDistance": 5,
	"gridUnits": "ft",
	"primaryTokenAttribute": "stress",
	"secondaryTokenAttribute": "void",
	"socket": true,
	"url": "https://github.com/Muttley/foundryvtt-dishonored",
	"manifest": "https://raw.githubusercontent.com/Muttley/foundryvtt-dishonored/master/system/system.json",
	"download": "https://github.com/Muttley/foundryvtt-dishonored/releases/latest/download/dishonored.zip",
	"background": "systems/FVTT-Dishonored/assets/images/dishonored-splash.webp",
	"license": "LICENSE.txt"
}
